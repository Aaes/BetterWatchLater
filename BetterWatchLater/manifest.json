{
  "name": "Better watch later",
  "description": "Cleans up the 'Watch later' list on youtube",
  "version": "1.1",
  "manifest_version": 3,
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" },
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/playlist?list=WL"],
      "css": ["betterWatchLater.css"],
      "js": ["betterWatchLater.js"]
    }
  ]
}